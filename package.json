    "beautify": "eslint --fix ."
  },
  "dependencies": {
    "express": "~4.15.2",
    "config": "~1.26.1",
    "jade": "~1.11.0",
    "morgan": "~1.8.1",
    "probe-image-size": "~3.0.0",
    "sharp": "~0.17.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "codecov": "^2.2.0",
    "debug": "~2.6.3",
    "eslint": "^3.19.0",
    "eslint-plugin-es-beautifier": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.1",
    "nodemon": "^1.11.0"
  }
}
