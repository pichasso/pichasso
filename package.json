{
  "name": "pichasso",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "serve": "nodemon ./bin/www",
    "development": "nodemon --legacy-watch --inspect=0.0.0.0:9229 ./bin/www",
    "lint": "eslint ./**/*.js",
    "mocha": "node ./node_modules/mocha/bin/mocha",
